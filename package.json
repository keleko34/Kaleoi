{
  "name": "kaleoi",
  "version": "1.5.44",
  "description": "Data management",
  "main": "kaleoi.min.js",
  "scripts": {
    "test": "node ./scripts/test.js",
    "build": "node ./scripts/build.js",
    "publish:npm": "node ./scripts/publish.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keleko34/kaleoi.git"
  },
  "keywords": [
    "kaleoi",
    "mvvm",
    "framework"
  ],
  "author": "keleko34",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/keleko34/kaleoi/issues"
  },
  "homepage": "https://github.com/keleko34/kaleoi#readme",
  "dependencies": {
    "czosnek": "1.5.43",
    "frytki": "1.5.43",
    "pikantny": "1.5.43",
    "solone": "1.5.43"
  },
  "devDependencies": {
    "benchmark": "2.1.4",
    "chai": "4.1.2",
    "connect": "3.6.5",
    "google-closure-compiler-js": "20180402.0.0",
    "lodash": "4.17.15",
    "mocha": "4.0.1",
    "opn": "5.4.0",
    "platform": "1.3.4",
    "serve-static": "1.13.1",
    "sinon": "4.1.2",
    "sinon-chai": "2.14.0"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "browser": true,
      "mocha": true
    },
    "globals": {
      "define": true,
      "module": true,
      "czosnek": true,
      "solone": true,
      "frytki": true,
      "kaleoi": true,
      "Promise": true,
      "node_config": true,
      "local_config": true,
      "__KaleoiExtensions__": true
    },
    "rules": {
      "no-console": [
        "warn",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ]
    },
    "parserOptions": {
      "ecmaVersion": 2017
    }
  }
}
